<!DOCTYPE html>
<html>
  <head>
    <title>Task4Itr</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application' %>
  </head>

  <body>
    <% if user_signed_in? %>
       <%= link_to 'Logout',destroy_user_session_path,method: :delete %>
    <% end %>
    <%= yield %>
  </body>
</html>
